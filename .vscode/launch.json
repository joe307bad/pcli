{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "pcli achievements:analyzer",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/bin/run",
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js"
            ],
            "args": [
                "achievements:analyzer"
            ],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "pcli achievements:changeset gen",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/bin/run",
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js"
            ],
            "args": [
                "achievements:changeset",
                "gen"
            ],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "pcli achievements:changset commit",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/bin/run",
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js"
            ],
            "args": [
                "achievements:changeset",
                "commit"
            ],
            "console": "integratedTerminal"
        }
    ]
}