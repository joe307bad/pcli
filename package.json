{
  "name": "pcli",
  "description": "Points CLI",
  "version": "0.0.3",
  "author": "Joe Badaczewski @joe307bad",
  "bin": {
    "pcli": "./bin/run"
  },
  "bugs": "https://github.com/joe307bad/pcli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "csv-parse": "^4.11.1",
    "csv-stringify": "^5.5.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.15.0",
    "readdir": "^1.0.2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "fp-ts": "^2.7.0",
    "globby": "^10",
    "husky": "^4.2.5",
    "io-ts": "^2.2.7",
    "mocha": "^5",
    "nyc": "^14",
    "points": "https://github.com/joe307bad/points",
    "remeda": "^0.0.20",
    "sync-dotenv": "^2.6.1",
    "ts-node": "^8",
    "tsyringe": "^4.3.0",
    "typescript": "3.9.6"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/joe307bad/pcli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "hooks": {
      "init": [
        "./lib/hooks/init/init"
      ]
    },
    "bin": "pcli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "joe307bad/pcli",
  "scripts": {
    "lint": "eslint ./src/**/*.ts",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "sync-env": "sync-dotenv"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn sync-env"
    }
  },
  "types": "lib/index.d.ts"
}
